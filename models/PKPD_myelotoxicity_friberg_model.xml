<?xml version="1.0" encoding="UTF-8"?>
<sbml xmlns="http://www.sbml.org/sbml/level3/version2/core" level="3" version="2">
  <model id="myelotoxicity_friberg_model_PD" timeUnits="hour">
    <listOfUnitDefinitions>
      <unitDefinition id="hour">
        <listOfUnits>
          <unit kind="second" exponent="1" scale="0" multiplier="3600"/>
        </listOfUnits>
      </unitDefinition>
      <unitDefinition id="per_hour">
        <listOfUnits>
          <unit kind="second" exponent="-1" scale="0" multiplier="3600"/>
        </listOfUnits>
      </unitDefinition>
      <unitDefinition id="per_litre">
        <listOfUnits>
          <unit kind="litre" exponent="-1" scale="0" multiplier="1"/>
        </listOfUnits>
      </unitDefinition>
      <unitDefinition id="per_mL">
        <listOfUnits>
          <unit kind="litre" exponent="-1" scale="-3" multiplier="1"/>
        </listOfUnits>
      </unitDefinition>
      <unitDefinition id="mg">
        <listOfUnits>
          <unit kind="gram" exponent="1" scale="-3" multiplier="1"/>
        </listOfUnits>
      </unitDefinition>
      <unitDefinition id="litre_per_mg">
        <listOfUnits>
          <unit kind="litre" exponent="1" scale="0" multiplier="1"/>
          <unit kind="gram" exponent="-1" scale="-3" multiplier="1"/>
        </listOfUnits>
      </unitDefinition>
    </listOfUnitDefinitions>
    <listOfParameters>
      <parameter id="circ_0" value="987" constant="true" units="per_mL"/>
      <parameter id="MTT" value="84.3" constant="true" units="hour"/>
      <parameter id="gamma" value="0.414" constant="true" units="dimensionless"/>
      <parameter id="slope" value="0.0837" constant="true" units="litre_per_mg"/>
      <parameter id="E_drug" value="0" constant="false" units="dimensionless"/>
      <parameter id="prol" value="987" units="per_mL" constant="false"/>
      <parameter id="T1" value="987" units="per_mL" constant="false"/>
      <parameter id="T2" value="987" units="per_mL" constant="false"/>
      <parameter id="T3" value="987" units="per_mL" constant="false"/>
      <parameter id="circ" value="987" units="per_mL" constant="false"/>
      <parameter id="clearance" name="clearance" value="14.38" constant="true" units="per_hour"/>
      <parameter id="k_peripheral" name="k_peripheral" value="11.25" constant="true" units="per_hour"/>
    </listOfParameters>
    
    <listOfCompartments>
      <compartment id="central" name="central" spatialDimensions="3" size="3.42" constant="true" units="litre"/>
      <compartment id="peripheral" name="peripheral" spatialDimensions="3" size="22.25" constant="true" units="litre"/>
    </listOfCompartments>

    <listOfSpecies>
      <species id="central_drug" name="central_drug" compartment="central" initialConcentration="0.0000000000001" substanceUnits="mg" hasOnlySubstanceUnits="false" boundaryCondition="false" constant="false"/>
      <species id="peripheral_drug" name="peripheral_drug" compartment="peripheral" initialConcentration="0" substanceUnits="mg" hasOnlySubstanceUnits="false" boundaryCondition="false" constant="false"/>
    </listOfSpecies>
    <listOfReactions>
      <reaction id="elimination" name="elimination" reversible="false">
        <listOfReactants>
          <speciesReference species="central_drug" constant="true"/>
        </listOfReactants>
        <kineticLaw>
          <math xmlns="http://www.w3.org/1998/Math/MathML">
            <apply>
              <times/>
              <ci>clearance</ci>
              <ci>central_drug</ci>
              <ci>central</ci>
            </apply>
          </math>
        </kineticLaw>
      </reaction>
      <reaction id="Central_to_Peripheral" name="Central_to_Peripheral" reversible="true">
        <listOfReactants>
          <speciesReference species="central_drug" constant="true"/>
        </listOfReactants>
        <listOfProducts>
          <speciesReference species="peripheral_drug" constant="true"/>
        </listOfProducts>
        <kineticLaw>
          <math xmlns="http://www.w3.org/1998/Math/MathML">
            <apply>
              <times/>
              <ci>k_peripheral</ci>
              <apply>
                <minus/>
                <apply>
                  <times/>
                  <ci>central_drug</ci>
                  <ci> central </ci>
                </apply>
                <apply>
                  <times/>
                  <ci>peripheral_drug</ci>
                  <ci>peripheral</ci>
                </apply>
              </apply>
            </apply>
          </math>
        </kineticLaw>
      </reaction>
    </listOfReactions>

    <listOfRules>
      <assignmentRule variable="E_drug">
        <math xmlns="http://www.w3.org/1998/Math/MathML">
          <apply>
            <times/>
            <ci> slope </ci>
            <ci> central_drug </ci>
          </apply>
        </math>
      </assignmentRule>
      <rateRule variable="prol">
        <math xmlns="http://www.w3.org/1998/Math/MathML">
          <apply>
            <minus/>
            <apply>
              <times/>
              <apply>
                <divide/>
                <cn> 4 </cn>
                <ci> MTT </ci>
              </apply>
              <ci> prol </ci>
              <apply>
                <minus/>
                <cn> 1 </cn>
                <ci> E_drug </ci>
              </apply>
              <apply>
              <power/>
                <apply>
                  <divide/>
                  <ci> circ_0 </ci>
                  <ci> circ</ci>
                </apply>
                <ci> gamma </ci>
              </apply>
            </apply>
            <apply>
              <times/>
              <apply>
                <divide/>
                <cn> 4 </cn>
                <ci> MTT </ci>
              </apply>
              <ci> prol </ci>
            </apply>
          </apply>
        </math>
      </rateRule>
      <rateRule variable="T1">
        <math xmlns="http://www.w3.org/1998/Math/MathML">
        <apply>
          <minus/>
          <apply>
            <times/>
            <apply>
              <divide/>
              <cn> 4 </cn>
              <ci> MTT </ci>
            </apply>
            <ci> prol </ci>
          </apply>
          <apply>
            <times/>
            <apply>
              <divide/>
              <cn> 4 </cn>
              <ci> MTT </ci>
            </apply>
            <ci> T1 </ci>
          </apply>
        </apply>
        </math>
      </rateRule>
      <rateRule variable="T2">
        <math xmlns="http://www.w3.org/1998/Math/MathML">
        <apply>
          <minus/>
          <apply>
            <times/>
            <apply>
              <divide/>
              <cn> 4 </cn>
              <ci> MTT </ci>
            </apply>
            <ci> T1 </ci>
          </apply>
          <apply>
            <times/>
            <apply>
              <divide/>
              <cn> 4 </cn>
              <ci> MTT </ci>
            </apply>
            <ci> T2 </ci>
          </apply>
        </apply>
        </math>
      </rateRule>
      <rateRule variable="T3">
        <math xmlns="http://www.w3.org/1998/Math/MathML">
        <apply>
          <minus/>
          <apply>
            <times/>
            <apply>
              <divide/>
              <cn> 4 </cn>
              <ci> MTT </ci>
            </apply>
            <ci> T2 </ci>
          </apply>
          <apply>
            <times/>
            <apply>
              <divide/>
              <cn> 4 </cn>
              <ci> MTT </ci>
            </apply>
            <ci> T3 </ci>
          </apply>
        </apply>
        </math>
      </rateRule>
      <rateRule variable="circ">
        <math xmlns="http://www.w3.org/1998/Math/MathML">
        <apply>
          <minus/>
          <apply>
            <times/>
            <apply>
              <divide/>
              <cn> 4 </cn>
              <ci> MTT </ci>
            </apply>
            <ci> T3 </ci>
          </apply>
          <apply>
            <times/>
            <apply>
              <divide/>
              <cn> 4 </cn>
              <ci> MTT </ci>
            </apply>
            <ci> circ </ci>
          </apply>
        </apply>
        </math>
      </rateRule>
    </listOfRules>
  </model>
